{
  "name": "product-tracker",
  "version": "1.0.0",
  "scripts": {
    "build": "tsc",
    "start:dev": "cross-env NODE_ENV=development ts-node -r dotenv/config src/server.ts",
    "start:dev:watch": "cross-env NODE_ENV=development ts-node-dev -r dotenv/config src/server.ts",
    "start:prod": "tsc && cross-env NODE_ENV=production node dist/src/server.js",
    "test": "cross-env DATABASE_URL=file:../test.db vitest --run",
    "test:watch": "cross-env DATABASE_URL=file:../test.db vitest",
    "test:migrate": "cross-env DATABASE_URL=file:../test.db npx prisma migrate reset",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint .  --ext .ts --fix",
    "format": "prettier --write . && npx prisma format",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/autoload": "^5.7.1",
    "@fastify/awilix": "^3.0.2",
    "@fastify/swagger": "^8.3.1",
    "@fastify/swagger-ui": "^1.7.0",
    "@fastify/type-provider-typebox": "^3.0.0",
    "@prisma/client": "^4.12.0",
    "@sinclair/typebox": "^0.26.8",
    "awilix": "^8.0.1",
    "camelcase": "^6.3.0",
    "dotenv": "^16.0.3",
    "json-schema-faker": "^0.5.0-rcv.46",
    "luxon": "^3.3.0",
    "muninn": "^0.17.0",
    "pino": "^8.11.0",
    "toad-scheduler": "^2.2.0",
    "undici": "^5.21.0",
    "vitest": "^0.29.8",
    "vitest-mock-extended": "^1.1.3"
  },
  "devDependencies": {
    "@types/luxon": "^3.2.0",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vitest": "^0.0.57",
    "husky": "^8.0.3",
    "prettier": "^2.8.7",
    "prisma": "^4.12.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tslib": "^2.5.0",
    "typescript": "^5.0.2"
  }
}
